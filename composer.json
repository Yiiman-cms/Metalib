{
  "name": "yiiman/yii-lib-metalib",
  "description": "meta library for store your data in table like options,objects,strings and etc, that can related to another models",
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "autoload": {
    "psr-4": {
      "YiiMan\\YiiLibMeta\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "replace": {
    "bower-asset/jquery": ">=1.11.0",
    "bower-asset/inputmask": ">=3.2.0",
    "bower-asset/punycode": ">=1.3.0",
    "bower-asset/yii2-pjax": ">=2.0.0"
  },

  "authors": [
    {
      "name": "YiiMan",
      "email": "info@yiiman.ir",
      "homepage": "https://yiiman.ir"
    }
  ],
  "require": {
    "yiisoft/yii2": "*"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}